# MultiNIC Agent Configuration
# Copy this file to /etc/multinic-agent/config.env and update values

# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_USER=multinic
DB_PASSWORD=your_password_here
DB_NAME=multinic_db

# Agent Configuration
POLL_INTERVAL=30s
LOG_LEVEL=info
HEALTH_PORT=8080

# Performance Configuration
MAX_CONCURRENT_TASKS=5
BACKOFF_ENABLED=true
BACKOFF_MAX_INTERVAL=5m
BACKOFF_MULTIPLIER=2